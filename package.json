{
    "name": "svg",
    "displayName": "svg",
    "description": "SVG Language Support",
    "version": "0.0.1",
    "publisher": "jock",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages",
        "Linters",
        "Other"
    ],
    "activationEvents": [
        "onLanguage:xml"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.moveCursor",
            "title": "Move Cursor"
        },{
            "command": "extension.showSvg",
            "title": "Show SVG"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}